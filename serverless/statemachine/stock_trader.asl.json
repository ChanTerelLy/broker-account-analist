{
  "Comment": "This is your state machine",
  "StartAt": "Parallel",
  "States": {
    "Parallel": {
      "Type": "Parallel",
      "End": true,
      "Branches": [
        {
          "StartAt": "ETF",
          "States": {
            "ETF": {
              "Type": "Task",
              "Resource": "arn:aws:states:::lambda:invoke",
              "Parameters": {
                "Payload.$": "$",
                "FunctionName": "${EtfArn}"
              },
              "OutputPath": "$.Payload",
              "End": true
            }
          }
        },
        {
          "StartAt": "Bonds",
          "States": {
            "Bonds": {
              "Type": "Task",
              "Resource": "arn:aws:states:::lambda:invoke",
              "Parameters": {
                "Payload.$": "$",
                "FunctionName": "${BondsArn}"
              },
              "OutputPath": "$.Payload",
              "End": true
            }
          }
        },
        {
          "StartAt": "Shares",
          "States": {
            "Shares": {
              "Type": "Task",
              "Resource": "arn:aws:states:::lambda:invoke",
              "Parameters": {
                "Payload.$": "$",
                "FunctionName": "${SharesArn}"
              },
              "OutputPath": "$.Payload",
              "End": true
            }
          }
        },
        {
          "StartAt": "ForeignShares",
          "States": {
            "ForeignShares": {
              "Type": "Task",
              "Resource": "arn:aws:states:::lambda:invoke",
              "Parameters": {
                "Payload.$": "$",
                "FunctionName": "${ForeignSharesArn}"
              },
              "OutputPath": "$.Payload",
              "End": true
            }
          }
        },
        {
          "StartAt": "Coupons",
          "States": {
            "Coupons": {
              "Type": "Task",
              "Resource": "arn:aws:states:::lambda:invoke",
              "Parameters": {
                "Payload.$": "$",
                "FunctionName": "${CouponsArn}"
              },
              "OutputPath": "$.Payload",
              "End": true
            }
          }
        }
      ]
    }
  }
}